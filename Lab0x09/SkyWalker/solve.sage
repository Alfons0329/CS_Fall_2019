#!/usr/bin/env python2 (for sage)
# flag length is 27 words, and bruteforce 128 bytes!
# padding should be randomized
from Crypto.Util.number import *
n = 
c = 

P.<x> = PolynomialRing(Zmod(n))

# encrypt.py: if len(x) > 128 - 2, raise ValueError 
for l in range(1, 127):
    # padding should be nonidentical each time, just check encrypt.py
    pad = b'\x00' + b'\x87' * (126 - l) + b'\x01'
    pad = bytes_to_long(pad) << (8 * l)

    # p77 step 3
    f = (pad + x) ^ 3 - c
    
    # p77 step 4 x0 會是fx(x) \equiv (m~ + x) ^ e - c (mod N)小根
    roots = f.small_roots()
    if roots:
        root = roots[0]
        flag = long_to_bytes(root)
        if flag.startswith(b'FLAG'):
            print(flag)
            break
